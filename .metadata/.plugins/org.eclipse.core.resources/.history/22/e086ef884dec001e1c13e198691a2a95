package ch24;

public class Student {
	
	private static int serialNum = 1000;
	private int stuNum;
	private String stuName;
	
	
	public student() {}
	
	public static int getStuNum() {
		stuNum++;
		return stuNum;
	}
	public String getStuName() {
		return stuName;
	}
	public void setStuName(String stuName) {
		this.stuName = stuName;
	}
	

}//Student
